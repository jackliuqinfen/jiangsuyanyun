
{
  "routes": [
    {
      "match": "/api/kv*",
      "forward": "edge-functions/api/kv.js"
    },
    {
      "match": "/api/file*",
      "forward": "edge-functions/api/file.js"
    },
    {
      "match": "/*",
      "custom_handler": "static_storage",
      "fallback": "/index.html"
    }
  ],
  "headers": [
    {
      "match": "/images/*",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    }
  ]
}
